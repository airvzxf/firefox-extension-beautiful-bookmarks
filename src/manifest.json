{
  "manifest_version": 2,
  "name": "Efficient Tab Manager",
  "version": "0.1",
  "description": "Start to manage your tabs efficiently and increase your productivity today!",
  "homepage_url": "https://github.com/airvzxf/firefox-extension-efficient-tab-manager",
  "icons": {
    "48": "icons/logo-48px.png",
    "96": "icons/logo-96px.png"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/logo-48px.png",
    "default_title": "Efficient Tab Manager",
    "default_popup": "popup/scope.html"
  }
}